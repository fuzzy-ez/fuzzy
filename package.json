{
  "name": "@fuzzy/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@7.11.0",
  "license": "MIT",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "start": "pnpm run dev",
    "dev": "pnpm -r --parallel run stub",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "clean": "rimraf dist && pnpm -r --parallel clean",
    "play": "pnpm -C playground run dev"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@fuzzy/components": "workspace:*",
    "@fuzzy/utils": "workspace:*",
    "@iconify-json/carbon": "1.1.8",
    "@iconify-json/fluent-emoji": "1.1.1",
    "@pity/eslint-config": "0.1.9",
    "@types/node": "18.7.18",
    "@vitejs/plugin-vue-jsx": "2.0.1",
    "@vueuse/core": "9.2.0",
    "bumpp": "8.2.1",
    "eslint": "8.22.0",
    "esno": "0.16.3",
    "fuzzy-ui": "workspace:*",
    "husky": "8.0.1",
    "jsdom": "20.0.0",
    "lint-staged": "13.0.3",
    "npm": "8.19.2",
    "rimraf": "3.0.2",
    "tsup": "6.2.3",
    "typescript": "4.8.3",
    "unocss": "0.45.22",
    "unplugin-auto-import": "0.11.2",
    "unplugin-vue-components": "0.22.7",
    "unplugin-vue-macros": "0.11.2",
    "vite": "3.1.3",
    "vitepress": "1.0.0-alpha.15",
    "vitest": "0.23.4",
    "vue": "3.2.39"
  },
  "workspaces": [
    "packages/*",
    "docs",
    "play"
  ],
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}
