    "@vueuse/core": "^9.1.1",
    "bumpp": "^8.2.1",
    "eslint": "8.22.0",
    "esno": "^0.16.3",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.0.3",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "unocss": "^0.45.12",
    "unplugin-auto-import": "0.11.2",
    "unplugin-vue-components": "0.22.4",
    "vite": "^3.0.9",
