    "create": "fuzzy-cli create",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "unplugin-vue-define-options": "^0.12.6"
